{
  "version": 2,
  "env": {
    "PORT": "5000",
    "DB_URL": "@DB_URL",
    "JWT_SECRET": "SECRET_SENTENCE_291()"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "DELETE",
        "PUT"
      ]
    }
  ]
}